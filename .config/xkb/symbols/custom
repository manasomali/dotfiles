partial alphanumeric_keys

xkb_symbols "custom" {
    // base keymap
    include "pc+us(alt-intl)+inet(evdev)+capslock(escape)"

    // accents
    key <AC11> {
        type= "FOUR_LEVEL",
        symbols[Group1]= [ apostrophe, quotedbl, dead_diaeresis, dead_diaeresis ]
    };
    key <TLDE> {
        type= "FOUR_LEVEL",
        symbols[Group1]= [ dead_acute, dead_grave, asciitilde, grave ]
    };

    // altgr
    key <AB03> {
        type= "FOUR_LEVEL_SEMIALPHABETIC",
        symbols[Group1]= [ c, C, ccedilla, Ccedilla ]
    };
    key <PGDN> { [ End ] };
    key <PGUP> { [ Home ] };

};

